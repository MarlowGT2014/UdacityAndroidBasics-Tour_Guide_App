<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AttractionsActivity"
            android:label="Attractions in Atlanta"></activity>
        <activity android:name=".HistoryActivity"
            android:label="Outdoor Activities around Atlanta"></activity>
        <activity android:name=".MyFavoritesActivity"
            android:label="My Favorites Places in Atlanta"></activity>
        <activity android:name=".RestaurantsActivity"
            android:label="Great Restaurants in Atlanta"></activity>
    </application>

</manifest>